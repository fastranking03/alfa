<script>
    $(document).ready(function(){
        // Hide all sub-menus initially
        $('.sub-menu').hide();

        // Add click event listener to toggle sub-menu visibility
        $('.menu-item-has-children').click(function(){
            $(this).children('.sub-menu').slideToggle();
        });
    });
</script>
